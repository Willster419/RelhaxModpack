<local:RelhaxWindow x:Class="RelhaxModpack.Windows.Credits"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:local="clr-namespace:RelhaxModpack.Windows"
                    mc:Ignorable="d"
                    Title="Credits" Loaded="RelhaxWindow_Loaded"
                    Height="625" Width="600" MinHeight="100" MinWidth="100"
                    ApplyToolTips="False" LocalizeWindow="False" EscapeKeyClosesWindow="True">
    <Window.Resources>
        <Style TargetType="Grid" BasedOn="{StaticResource DarkThemeGrid}"/>
    </Window.Resources>
    <Grid>
        <TextBox x:Name="CreditsTextBox" HorizontalAlignment="Stretch" Margin="10,10,10,10" TextWrapping="Wrap" VerticalAlignment="Stretch" AcceptsReturn="True" AcceptsTab="True" IsReadOnly="True" VerticalScrollBarVisibility="Visible"/>
    </Grid>
</local:RelhaxWindow>
